<div>
	<% provide(:title, 'Team') %>
	<div class="tabbable tabs-left">
		<ul class="nav nav-tabs">
			<li class="nav-header">Team Members</li>
			<% @users.each do |user| %>
				<li>
					<%= link_to user.first, "#" + user.id.to_s, :data => {:toggle=>"tab"} %>
				</li>
			<% end %>
		</ul>
		<div class="tab-content">
			<% @users.each do |user| %>
				<div class="tab-pane fade in" id=<%= user.id.to_s %> >
					<h3> <%= user.first + " " + user.last %> </h3>
					<h4> <%= user.role %> </h4>
					<p>Contact: <%= user.email %></p>
					<p>Url: <%= user.url %></p>	
				</div>
			<% end %>
		</div>
	</div>
</div>
